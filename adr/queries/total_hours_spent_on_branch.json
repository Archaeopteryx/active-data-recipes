{
    "limit":1000,
    "from":"task",
    "where":{"and":[
        {"eq":{"repo.branch.name":"%(branch_name)s"}},
        {"gte":{"repo.push.date":{"date":"%(from_date)s"}}},
        {"lte":{"repo.push.date":{"date":"%(to_date)s"}}}
    ]},
    "select":[
        {
            "aggregate":"sum",
            "name":"hours",
            "value":{"div":{"action.duration":"%(max_task_duration)s"}}
        },
        {"name":"tasks", "aggregate":"count"}
    ]
}
